# n8n local environment
# Required base config
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/
N8N_EDITOR_BASE_URL=http://localhost:5678/
N8N_ENCRYPTION_KEY=change-this-key-please-32chars-min-123456
# Disable telemetry for local
N8N_DIAGNOSTICS_ENABLED=false
# Enable external modules in Function/Code nodes if needed
NODE_FUNCTION_ALLOW_EXTERNAL=axios,cheerio
# Security/auth
N8N_BASIC_AUTH_ACTIVE=false
N8N_USER_MANAGEMENT_DISABLED=true
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
# DB + Task runners (recommended by latest n8n)
DB_SQLITE_POOL_SIZE=2
N8N_RUNNERS_ENABLED=true
# Timezone
GENERIC_TIMEZONE=UTC
